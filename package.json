{
  "name": "blog3-front",
  "author": "寒枫",
  "description": "寒枫的个人博客",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev:both": "concurrently \"next dev\" \"npm run dev:electron\"",
    "dev": "next dev",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "buildDev": "next build --turbopack",
    "build": "next build",
    "start": "next start",
    "dist": "npm run build && electron-builder",
    "electron:generate-icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "lint": "eslint"
  },
  "build": {
    "productName": "寒枫的博客",
    "appId": "www.hanphone.top",
    "asar": true,
    "files": [
      "main.js",
      "public/**/*",
      ".next",
      "next.config.ts",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build/icon.png"
    },
    "win": {
      "icon": "build/icon.png"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.0",
    "@uiw/react-md-editor": "^4.0.8",
    "antd": "^5.27.1",
    "axios": "^1.11.0",
    "cheerio": "^1.1.0",
    "clipboard": "^2.0.11",
    "compressorjs": "^1.2.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "echarts": "^6.0.0",
    "highlight.js": "^11.11.1",
    "isomorphic-dompurify": "^2.26.0",
    "katex": "^0.16.25",
    "lucide-react": "^0.541.0",
    "md5": "^2.3.0",
    "next": "^15.5.7",
    "pg": "^8.16.3",
    "pixi-live2d-display": "^0.4.0",
    "pixi.js": "^8.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-player": "^3.3.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-rewrite": "^4.0.2",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.19.19",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "concurrently": "^9.2.1",
    "electron": "^39.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "react-syntax-highlighter": "^15.6.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
